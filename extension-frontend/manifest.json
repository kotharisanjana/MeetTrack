{
    "manifest_version": 3,
    "name": "MeetTrack",
    "version": "1.0",
    "description": "MeetTrack meeting assistant",
    "permissions": ["tabCapture", "offscreen"],
    "background": {
        "service_worker": "service-worker.js"
    },
    "action": {
        "default_popup": "extension.html",
        "default_icon": {
            "16": "icon.png"
        }
    },
    "icons": {
        "16": "icon.png"
    },
    "host_permissions": [
        "http://ec2-3-17-39-185.us-east-2.compute.amazonaws.com:5000/"
    ]
}